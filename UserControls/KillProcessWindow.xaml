<Window x:Name="Kill_Processes" x:Class="UserControls.KillProcessWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="KillProcessWindow" Height="300" Width="300">
    <Grid>


        <Button IsEnabled="False" Name="btnKill" Content="Kill" HorizontalAlignment="Left" Margin="218,10,0,0" VerticalAlignment="Top" Width="75" Click="btnKill_Click"/>
        <ListBox Name="lstbxProcesses" HorizontalAlignment="Left" Height="251" Margin="10,10,0,0" VerticalAlignment="Top" Width="203" SelectionChanged="lstbxProcesses_SelectionChanged"/>
    </Grid>
</Window>
