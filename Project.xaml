<Window x:Class="LabRun.Project"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Project" Height="396.02" Width="345.771">
    <Grid>
        <ListBox Name="lstProjects" HorizontalAlignment="Left" Height="244" Margin="10,43,0,0" VerticalAlignment="Top" Width="291" SelectionChanged="lstProjects_SelectionChanged" PreviewMouseDoubleClick="lstProjects_PreviewMouseDoubleClick"/>
        <Label Content="Select Your Project:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button IsEnabled="False" Name="btnOK" Content="OK" HorizontalAlignment="Left" Margin="10,300,0,0" VerticalAlignment="Top" Width="75" Click="btnOK_Click"/>
        <Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="226,300,0,0" VerticalAlignment="Top" Width="75" Click="btnCancel_Click"/>

    </Grid>
</Window>
