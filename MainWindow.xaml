<Window x:Class="LabRun.MainWindow"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Browse" Height="30" HorizontalAlignment="Left" Margin="155,15,0,0" Name="button1" VerticalAlignment="Top" Width="76" Click="button1_Click" />
        <Label Content="..." Height="28" HorizontalAlignment="Left" Margin="265,15,0,0" Name="label1" VerticalAlignment="Top" />
        <ListBox Height="196" SelectionMode="Multiple" HorizontalAlignment="Left" Margin="12,15,0,0" Name="listBox1" VerticalAlignment="Top" Width="131" />
        <Button IsEnabled='False' Content="Run" Height="28" HorizontalAlignment="Left" Margin="156,57,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button2_Click" />
        <Button IsEnabled='True' Content="Kill" Height="28" HorizontalAlignment="Left" Margin="265,57,0,0" Name="btnKill" VerticalAlignment="Top" Width="75" Click="btnKill_Click" />
    </Grid>
</Window>
